set basename_idx = func(string)
  set found = false
  set idx = 0
  set i = @length(string) - 1

  while* not found and i >= 0 do
    if string[i] = "/" then
      set idx = i + 1
      set found = true
    end
    set i = i - 1
  end

  idx
end

set file_name = @readln()
set base = ""

for* i = basename_idx(file_name), @length(file_name) do
  set base = base .. file_name[i]
end

@writeln(base)
