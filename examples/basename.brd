set basename = func(string)
  set found = false
  set idx = 0
  set i = @length(string) - 1

  while* not found and i >= 0 do
    if string[i] = "/" then
      set idx = i + 1
      set found = true
    end
    set i = i - 1
  end

  idx
end

set file_name = @readln()
set idx = basename(file_name)
set base = ""

while* idx < @length(file_name) do
  set base = base .. file_name[idx]
  set idx = idx + 1
end

@writeln(base)
