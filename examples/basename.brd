set basename = func(string)
  set idx = 0
  set i = 0
  while* i < @length(string) do
    if string[i] = "/" then
      set idx = i + 1
    end
    set i = i + 1
  end

  idx
end

set file_name = @readln()
set idx = basename(file_name)
set base = ""

while* idx < @length(file_name) do
  set base = base .. file_name[idx]
  set idx = idx + 1
end

@writeln(base)
