set make_adder = func(a)
  func(b)
    a + b
  end
end

set add_5 = make_adder(5)

@writeln(add_5(20.2))

@writeln(make_adder(14)(6))

# use self for recursive functions
set a = func(m, n)
  if m = 0 then
    n + 1
  elif n = 0 then
    self(m - 1, 1)
  else
    self(m - 1, self(m, n - 1))
  end
end

@writeln(a(3, 3))
